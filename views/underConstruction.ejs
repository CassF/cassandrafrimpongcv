<body>
<!-- Start of banner -->
<div class="banner">
    <h1>Cassandra Frimpong</h1>
    <h2><strong> WEB DEVELOPER</strong></h2>
</div>
<!-- End of Banner -->
<!-- Start of Contact section -->
<section class="section greySection" id="contact">
    <h3>Contact</h3>
    <div class="row container">
        <form class="col s12" method="POST" action="/contact">
            <div class="row">
                <div class="input-field col s6">
                    <input placeholder="First Name" name="firstName" id="firstName" type="text" class="validate" required>
                    <label for="firstName">First Name</label>
                </div>
                <div class="input-field col s6">
                    <input placeholder="Last Name" name="lastName" id="lastName" type="text" class="validate" required>
                    <label for="lastName">Last Name</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <input placeholder="Email" name="email" id="email" type="email" class="validate" required>
                    <label for="email">Email</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s12">
                    <input placeholder="Message" name="message" id="message" type="text" class="validate" required>
                    <label for="message">Message</label>
                </div>
            </div>
            <div class="row">
                <div>
                    <button class="btn" type="submit" name="action" onclick="Materialize.toast('Thank you! Your message has sent.', 3000)">
                        <i class="fa fa-envelope-o"></i> Send Message
                    </button>
                </div>
            </div>
        </form>
    </div>
    <!-- Back to top -->
<a href="#" class="go-top"><i class="fa fa-3x fa-chevron-up " aria-hidden="true"></i></a>
</section>
